    <system:String x:Key="CONTROLLER_OPTIONS_COPYUNENC">Copy unencrypted files too</system:String>
    <system:String x:Key="CONTROLLER_OPTIONS_INFEREXT">Infer an extension of a file when the file is invalid</system:String>

