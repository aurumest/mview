    <system:String x:Key="EXPLORER_ALERT_DIR_NOT_FOUND">The directory does not found. Please try again.</system:String>
    <system:String x:Key="EXPLORER_ALERT_ROOT_DIR_SELECTED">The root directory cannot be loaded.</system:String>

    <system:String x:Key="EXPLORER_OPEN_FILES_DIALOG_TITLE">Please select files.</system:String>
